{
  "type": "base",
  "args": {
    "epochs": 100,
    "patience": 15,
    "optimizer": {
      "type": "adam",
      "args": {
        "lr": 1e-3
      }
    }
  },
  "regularization": [
    {
      "type": "L2",
      "args": {
        "lambda_reg": 0.001,
        "exclusions": [
          "bias"
        ]
      }
    }
  ]
}